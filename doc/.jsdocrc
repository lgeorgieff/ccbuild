{
  "tags": {
    "allowUnknownTags": false,
    "dictionaries": [
      "jsdoc",
      "closure"
    ]
  },
  "source": {
    "includePattern": ".+\\.js(doc)?$",
    "include": [
      "package.json",
      "README.md"
    ],
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "plugins": [
    "tools/jsdoc_plugins/closure"
  ],
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": false
  },
  "opts": {
    "encoding": "utf8",
    "destination": "./doc/gen/",
    "recurse": true
  }
}
