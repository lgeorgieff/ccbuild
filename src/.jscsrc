{
    "disallowMultipleSpaces": true,
    "disallowMultipleLineBreaks": true,
    "validateLineBreaks": "LF",
    "disallowTabs": true,
    "validateIndentation": 4,
    "validateQuoteMarks": "'",
    "disallowTrailingWhitespace": true,
    "maximumLineLength": {
        "value": 120,
        "allExcept": ["comments", "regex"]
    },
    "requireBlocksOnNewline": {
        "minLines": 1,
        "includeComments": true
    },
    "requireLineFeedAtFileEnd": true,
    "requirePaddingNewLinesAfterUseStrict": true,
    "requireSemicolons": true,
    "requireOperatorBeforeLineBreak": true,
    "disallowNewlineBeforeBlockStatements": true,
    "disallowPaddingNewlinesInBlocks": true,
    "requireAlignedMultilineParams": "firstParam",
    "validateParameterSeparator": ", ",
    "requireCamelCaseOrUpperCaseIdentifiers": true,
    "requireCapitalizedConstructors": true,
    "requireParenthesesAroundIIFE": true,
    "requireSpaceAfterBinaryOperators": true,
    "requireCommaBeforeLineBreak": true,
    "disallowTrailingComma": true,

    "requireSpaceBeforeBlockStatements": true,
    "requireSpaceBeforeObjectValues": true,
    "requireSpacesInForStatement": true,
    "requireSpacesInFunction": {
        "beforeOpeningRoundBrace": true,
        "beforeOpeningCurlyBrace": true
    },
    "requireSpacesInConditionalExpression": {
        "afterTest": true,
        "beforeConsequent": true,
        "afterConsequent": true,
        "beforeAlternate": true
    },
    "requireSpaceAfterComma": true,
    "requireSpaceAfterKeywords": [
        "case",
        "catch",
        "do",
        "else",
        "for",
        "function",
        "if",
        "while",
        "return",
        "switch",
        "try",
        "typeof",
        "void"
    ],
    "requireSpaceBeforeBinaryOperators": [
        "=", "+=", "-=", "*=", "/=", "%=", "<<=", ">>=", ">>>=",
        "&=", "|=", "^=", "+=",
        "+", "-", "*", "/", "%", "<<", ">>", ">>>", "&",
        "|", "^", "&&", "||", "===", "==", ">=",
        "<=", "<", ">", "!=", "!=="
    ],
    "disallowSpaceAfterPrefixUnaryOperators": true,
    "disallowSpaceBeforePostfixUnaryOperators": true,
    "disallowSpaceBeforeComma": true,
    "disallowSpacesInsideBrackets": true,
    "disallowSpacesInsideObjectBrackets": true,
    "disallowSpacesInsideParentheses": true,
    "disallowSpaceAfterObjectKeys": {"allExcept": ["method"]},

    "disallowMultipleLineStrings": true,
    "disallowMultiLineTernary": true,
    "disallowMultipleVarDecl": true,
    "disallowEmptyBlocks": {"allExcept": ["comments"]},
    "disallowKeywords": ["with", "class", "debugger", "goto"],
    "disallowNestedTernaries": true,
    "disallowNodeTypes": ["LabeledStatement"],
    "safeContextKeyword": ["self"],
    "requireDotNotation": true,

    "requireSpread": true,
    "requireTemplateStrings": true,

    "jsDoc": {
        "checkAnnotations": "jsdoc3",
        "enforceExistence": {"allExcept": ["exports"]},
        "checkParamExistence": true,
        "checkParamNames": true,
        "requireParamTypes": true,
        "checkRedundantParams": true,
        "checkReturnTypes": true,
        "checkRedundantReturns": true,
        "requireReturnTypes": true,
        "checkTypes": true,
        "requireNewlineAfterDescription": true,
        "checkRedundantAccess": "enforceLeadingUnderscore",
        "leadingUnderscoreAccess": "protected",
        "requireDescriptionCompleteSentence": true,
        "requireParamDescription": true,
        "requireReturnDescription": true

    }
}
