  "bugs": {
    "url": "https://github.com/lgeorgieff/js_base/issues"
  },
  "homepage": "https://github.com/lgeorgieff/js_base#readme",
  "engines": {
    "node": ">=4.0.0"
  },
  "devDependencies": {
    "jscs": "^2.4.0"
  },
  "scripts": {
    "postinstall": "npm run get_git_dependencies && npm run compile_closure_compiler",
    "get_git_dependencies": "git submodule init && git submodule update",
    "postget_git_dependencies": "cd tools/node.js-closure-compiler-externs/ && git apply ../node.js_closure_compiler_externs_annotation_fixes.path 2>/dev/null || true",
    "compile_closure_compiler": "if [ ! -e tools/closure_compiler/target/closure-compiler-1.0-SNAPSHOT.jar ]; then mvn clean install -DskipTests --file tools/closure_compiler/pom.xml; fi",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "if [ $(find src -name '*.js' | wc -l) -gt 0 ]; then jscs --config src/.jscsrc src/*.js; fi",
    "precompile": "npm run compile_closure_compiler",
    "compile": "java -jar tools/closure_compiler/target/closure-compiler-1.0-SNAPSHOT.jar --flagfile src/.closure_compiler.flagfile --js src/ > /dev/null",
    "prebuid": "npm run lint",
    "validate": "npm run compile"
  }
}
